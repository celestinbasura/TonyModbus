<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.celeMC.tonymodbus.app" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/light_bulb"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.InteriorPointActivity"
            android:label="@string/title_activity_interior_point" >
        </activity>
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.ExteriorPointActivity"
            android:label="@string/title_activity_exterior_point" >
        </activity>
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.GroupPointActivity"
            android:label="@string/title_activity_group_point" >
        </activity>
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.ListActivity"
            android:label="@string/title_activity_list_activity_test" >
        </activity>
        <activity
            android:name="com.celeMC.tonymodbus.app.Activities.ListActivityNew"
            android:label="@string/title_activity_list_activity_new" >
        </activity>
    </application>

</manifest>
